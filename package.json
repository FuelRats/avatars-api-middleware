{
  "name": "@fuelrats/next-adorable-avatars",
  "version": "0.3.0",
  "description": "Adorable avatar API routes for Next.js",
  "engines": {
    "node": "^14.15.0 | >=16.0"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "test:mocha": "mocha --exit",
    "test": "$npm_execpath run build && $npm_execpath run test:mocha && $npm_execpath run lint",
    "build:assets": "cp -r src/img dist/img",
    "build": "rm -rf dist && tsc && $npm_execpath run build:assets",
    "dev:lint": "onchange -i -k 'src/**/*.ts' -- $npm_execpath run lint",
    "dev:server": "onchange -i -k 'src/**/*.ts' -- ts-node test/server.ts",
    "dev": "concurrently --kill-others --prefix=name --names=server,eslint --prefix-colors=green,magenta \"$npm_execpath run dev:server\" \"$npm_execpath run dev:lint\"",
    "prepack": "$npm_execpath run build",
    "prepublish": "$npm_execpath run test"
  },
  "author": "",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FuelRats/avatars-api-middleware.git"
  },
  "dependencies": {
    "avatars-utils": "0.0.4",
    "next-connect": "^0.10.1",
    "pure-uuid": "^1.6.2",
    "sharp": "^0.28.3",
    "workerpool": "^6.1.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.19",
    "@types/koa": "^2.13.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/serve-favicon": "^2.5.2",
    "@types/sharp": "^0.28.3",
    "@types/supertest": "^2.0.11",
    "@types/workerpool": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "chai": "^4.3.4",
    "concurrently": "^6.2.0",
    "eslint": "^7.29.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.0",
    "mocha": "^9.0.1",
    "next": "9.5.0",
    "onchange": "^7.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "supertest": "^6.1.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "peerDependencies": {
    "next": ">=9.5.0"
  }
}
